      "datasource": "redshift",
      "query": "SELECT TO_CHAR(VISIT_DATE, 'dd/mm/yyyy') AS VISIT_DATE_TEXT, DESCRIPTION, VISIT_TYPE, COUNT(DISTINCT OFFENDER_VISIT_ID) AS V_COUNT_VISITS, COUNT(DISTINCT OFFENDER_VISIT_VISITOR_ID) AS V_COUNT_VISITOR_ID FROM ${tableId} GROUP BY TO_CHAR(VISIT_DATE, 'dd/mm/yyyy'), DESCRIPTION, VISIT_TYPE",

{
  "id": "FmFV5VvinwQAt_QBAHB7sV8FACJIABRd",
  "name": "ORS Visits Booked for a Date Range in the Future",
  "description": "A list of all Prisoners to be visited for an Establishment and date range selected at runtime. The report shows a summary total of prisoners to be visited, along with number of visitors per day, broken down by Official/Social. A detail tab also shows the prisoners who are to be visited VIS0022 - v00.00.02 Last Modified: 20/07/2022 ",
  "metadata": {
    "version": "1.0.0",
    "tags": ["VIS0022 v00.00.02"]
  },
  "datasource": [
    {
      "id": "nomis",
      "name": "NOMIS",
      "database": "DIGITAL_PRISON_REPORTING",
      "catalog": "nomis"
    }
  ],
  "dataset": [
    {
      "id": "3376551/DP3",
      "name": "Visits",
      "description": "Visits",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT AT_OFFENDER_VISITS.VISIT_DATE AS VISIT_DATE, TO_CHAR(AT_OFFENDER_VISITS.VISIT_DATE, 'dd/mm/yyyy') AS VISIT_DATE_TEXT, AT_VISIT_LOCATION.DESCRIPTION AS DESCRIPTION, AT_VISIT_LOCATION.INTERNAL_LOCATION_TYPE AS INTERNAL_LOCATION_TYPE, AT_OFFENDER_VISITS.START_TIME AS START_TIME, AT_OFFENDER_VISITS.END_TIME AS END_TIME, CASE WHEN AT_OFFENDER_VISITS.VISIT_TYPE = 'OFFI' THEN 'Official' ELSE 'Social/Family' END AS VISIT_TYPE, AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, DT_ALL_VISITORS.OFFENDER_VISIT_ID AS OFFENDER_VISIT_ID, DT_ALL_VISITORS.OFFENDER_VISIT_VISITOR_ID AS OFFENDER_VISIT_VISITOR_ID FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_OFFENDERS_LOCATIONS, OMS_OWNER.CASELOAD_AGENCY_LOCATIONS AT_CASELOAD_LOCATIONS, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_VISIT_LOCATION, OMS_OWNER.OFFENDER_VISITS AT_OFFENDER_VISITS, OMS_OWNER.OFFENDER_VISIT_VISITORS DT_ALL_VISITORS WHERE (DT_ALL_VISITORS.OFFENDER_VISIT_ID (+) = AT_OFFENDER_VISITS.OFFENDER_VISIT_ID) AND (AT_OFFENDER_VISITS.AGY_LOC_ID = AT_VISIT_LOCATION.AGY_LOC_ID AND AT_OFFENDER_VISITS.VISIT_INTERNAL_LOCATION_ID = AT_VISIT_LOCATION.INTERNAL_LOCATION_ID) AND (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDERS_LOCATIONS.AGENCY_LOCATION_TYPE = 'INST') AND (AT_CASELOAD_LOCATIONS.AGY_LOC_ID = AT_OFFENDER_BOOKING.AGY_LOC_ID) AND (AT_CASELOAD_LOCATIONS.AGY_LOC_ID = AT_OFFENDERS_LOCATIONS.AGY_LOC_ID (+)) AND (NOT AT_CASELOAD_LOCATIONS.AGY_LOC_ID IN ('OUT', 'TRN') AND NOT AT_CASELOAD_LOCATIONS.CASELOAD_ID IN ('ADMINC', 'CADM_I', 'MULTI')) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_VISITS.OFFENDER_BOOK_ID) AND (AT_OFFENDER_VISITS.VISIT_INTERNAL_LOCATION_ID = AT_VISIT_LOCATION.INTERNAL_LOCATION_ID) AND (AT_OFFENDER_VISITS.VISIT_STATUS = 'SCH') AND ((AT_OFFENDERS_LOCATIONS.AGY_LOC_ID IN (SELECT establishment_code FROM prompt_) OR ((SELECT establishment_code FROM prompt_) = 'All') /* END Filter: Hierarchies/Establishment/''Establishment Code'' */) /* START Filter: Hierarchies/Establishment/''Establishment Code'' */ AND (AT_OFFENDER_VISITS.VISIT_STATUS <> 'CANC') AND (TRUNC(AT_OFFENDER_VISITS.VISIT_DATE) >= (SELECT start_date FROM prompt_) AND TRUNC(AT_OFFENDER_VISITS.VISIT_DATE) <= (SELECT end_date FROM prompt_)) AND (AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y'))), dataset_ AS (SELECT VISIT_DATE, VISIT_DATE_TEXT, DESCRIPTION, INTERNAL_LOCATION_TYPE, START_TIME, END_TIME, VISIT_TYPE, OFFENDER_ID_DISPLAY, LAST_NAME, FIRST_NAME, OFFENDER_VISIT_ID, OFFENDER_VISIT_VISITOR_ID FROM dataset_base_)",
      "parameters": [
        {
          "index": 0,
          "name": "establishment_code",
          "filterType": "text",
          "reportFieldType": "string",
          "display": "Establishment Code",
          "description": "@Prompt('Establishment Code','A','Establishment\\Establishment Code',mono,free,,Not_Persistent,,User:3)",
          "mandatory": "true"
        },
        {
          "index": 1,
          "name": "start_date",
          "filterType": "date",
          "reportFieldType": "date",
          "display": "Enter Date(Start):",
          "description": "@prompt('Enter Event Date(Start):','D',,Mono,Free,Not_Persistent,,User:2)",
          "mandatory": "true"
        },
        {
          "index": 2,
          "name": "end_date",
          "filterType": "date",
          "reportFieldType": "date",
          "display": "Enter Date(End):",
          "description": "@prompt('Enter Event Date(End):','D',,Mono,Free,Not_Persistent,,User:3)",
          "mandatory": "true"
        }
      ],

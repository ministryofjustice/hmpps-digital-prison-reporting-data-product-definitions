{
  "id": "missing-ethnicity-metrics",
  "name": "Missing Ethnicity Metrics",
  "description": "Reports about Missing Ethnicity Metrics",
  "metadata": {
    "author": "Adam",
    "version": "1.2.3",
    "owner": "Eve"
  },
  "datasource": [
    {
      "id": "datamart",
      "name": "datamart"
    }
  ],
  "dataset": [
    {
      "id": "missing-ethnicity-dataset",
      "name": "Missing Ethnicity By Establishment Dataset",
      "datasource": "datamart",
      "query": "SELECT t1.est_id AS establishment_id,  ROUND(t2.missing_ethnicity_count * 100.0/t1.n, 2) AS missing_ethnicity_percentage, 100 - missing_ethnicity_percentage AS present_ethnicity_percentage   FROM (SELECT establishment_id AS est_id, COUNT(*) AS n  FROM \"datamart\".\"datamart\".\"prisoner_prisoner\"   WHERE establishment_id not in ('TRN', 'ZZGHI', 'OUT')  GROUP BY est_id) AS t1,  (SELECT establishment_id AS est_id, COUNT(*) AS missing_ethnicity_count  FROM \"datamart\".\"datamart\".\"prisoner_prisoner\"   WHERE establishment_id not in ('TRN', 'ZZGHI', 'OUT')  AND ethnicity_code is NULL  GROUP BY est_id) AS t2  WHERE t1.est_id = t2.est_id",
      "schema": {
        "field": [
          {
            "name": "establishment_id",
            "type": "string",
            "display": "Establishment ID"
          },
          {
            "name": "missing_ethnicity_percentage",
            "type": "long",
            "display": "Missing Ethnicity Percentage"
          },
          {
            "name": "present_ethnicity_percentage",
            "type": "long",
            "display": "present Ethnicity Percentage"
          }
        ]
      }
    }
  ],
  "dashboards": [
    {
      "id": "test-dashboard-1",
      "name": "Test Dashboard 1",
      "description": "Test Dashboard 1 Description",
      "metrics": [
        {
          "id": "missing-ethnicity-metric",
          "visualisationType": ["bar"]
        }
      ]
    }
  ],
  "metrics": [
    {
      "id": "missing-ethnicity-metric",
      "name": "Missing Ethnicity By Establishment Metric",
      "dataset": "missing-ethnicity-dataset",
      "display": "Missing Ethnicity By Establishment Metric",
      "description": "Missing Ethnicity By Establishment Metric",
      "visualisationType": ["bar", "doughnut"],
      "specification": [
        {
          "name": "establishment_id",
          "display": "Establishment ID"
        },
        {
          "name": "missing_ethnicity_percentage",
          "display": "Missing Ethnicity Percentage",
          "unit": "percentage"
        },
        {
          "name": "present_ethnicity_percentage",
          "display": "Present Ethnicity Percentage",
          "unit": "percentage"
        }
      ]
    }
  ],
  "policy": [
    {
      "id": "caseload",
      "type": "row-level",
      "action": ["(establishment_id='${caseload}')"],
      "rule": [
        {
          "effect": "permit",
          "condition": [
            {
              "match": ["${role}", "ROLE_PRISONS_REPORTING_USER"]
            }
          ]
        }
      ]
    }
  ],
  "report": [
    {
      "id": "missing-ethnicity",
      "name": "Missing Ethnicity",
      "description": "Missing Ethnicity Report",
      "created": "2024-09-16T09:00:00.000Z",
      "classification": "report classification",
      "version": "1.2.3",
      "dataset": "$ref:missing-ethnicity-dataset",
      "policy": [],
      "render": "HTML",
      "feature": [
        {
          "type": "print"
        }
      ],
      "specification": {
        "template": "list",
        "field": [
          {
            "name": "$ref:establishment_id",
            "sortable": true,
            "defaultsort": false
          },{
            "name": "$ref:missing_ethnicity_percentage",
            "sortable": true,
            "defaultsort": true
          },{
            "name": "$ref:present_ethnicity_percentage",
            "sortable": true,
            "defaultsort": false
          }
        ]
      },
      "destination": []
    }
  ]
}

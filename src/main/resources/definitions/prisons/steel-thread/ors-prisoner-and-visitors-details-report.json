      "description": "Visits",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT AT_OFFENDERS_LOCATIONS.AGY_LOC_ID AS AGY_LOC_ID, AT_OFFENDER_VISITS.AGY_LOC_ID AS SCHEDULED_VISIT_ESTABLISHMEN34, AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_PERSONS2.FIRST_NAME AS VPIS_FIRST_NAME, AT_PERSONS2.LAST_NAME AS VPIS_LAST_NAME, CASE WHEN AT_OFFENDER_VISITS.VISIT_TYPE = 'OFFI' THEN 'Official' ELSE 'Social/Family' END AS VISIT_TYPE, AT_PERSONS2.PERSON_ID AS PERSON_ID, AT_PERSONS2.BIRTHDATE AS BIRTHDATE, AT_VISIT_LOCATION.DESCRIPTION AS DESCRIPTION, AT_OFFENDER_VISITS.VISIT_DATE AS VISIT_DATE, AT_PERSON_ADDRESSES.STREET AS STREET, AT_PERSON_ADDRESSES.POSTAL_CODE AS POSTAL_CODE, SUBSTR(AT_AGENCY_INTERNAL_LOCATION_L4.DESCRIPTION, LENGTH(AT_AGENCY_INTERNAL_LOCATION_L4.AGY_LOC_ID) + 2, LENGTH(AT_AGENCY_INTERNAL_LOCATION_L4.DESCRIPTION)) AS UNIT_DESCRIPTION_4_SHORT, SUBSTR(AT_OFFENDER_VISIT_ORDERS.VISIT_ORDER_NUMBER || '', 9, 5) AS VISIT_ORDER_NUMBER_LAST_5_DI32, AT_OFFENDER.BIRTH_DATE AS BIRTH_DATE, AT_OFFENDER_VISITS.START_TIME AS START_TIME, AT_OFFENDER_VISITS.END_TIME AS END_TIME, AT_OFFENDER_VISITS.COMMENT_TEXT AS COMMENT_TEXT, AT_PERSON_ADDRESSES.PRIMARY_FLAG AS PRIMARY_FLAG, AT_PERSON_ADDRESSES.PREMISE AS PREMISE, AT_PERSON_ADDRESSES.COUNTY_CODE AS COUNTY_CODE FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_AGENCY_INTERNAL_LOCATION_L4, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_OFFENDERS_LOCATIONS, OMS_OWNER.AGENCY_LOCATIONS AT_WING_ESTABLISHMENT, OMS_OWNER.CASELOAD_AGENCY_LOCATIONS AT_CASELOAD_LOCATIONS, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_VISIT_LOCATION, USER_ACCESSIBLE_CASELOADS, OMS_OWNER.OFFENDER_VISITS AT_OFFENDER_VISITS, OMS_OWNER.PERSONS AT_PERSONS2, ADDRESSES AT_PERSON_ADDRESSES, USER_ACCESSIBLE_CASELOADS WING_CASELOAD_SECURITY, OFFENDER_VISIT_ORDERS AT_OFFENDER_VISIT_ORDERS, OMS_OWNER.OFFENDER_VISIT_VISITORS AT_OFFENDER_VISIT_VISITORS WHERE (AT_OFFENDER_VISIT_ORDERS.OFFENDER_VISIT_ORDER_ID (+) = AT_OFFENDER_VISITS.OFFENDER_VISIT_ORDER_ID) AND (AT_OFFENDER_VISITS.AGY_LOC_ID = AT_VISIT_LOCATION.AGY_LOC_ID AND AT_OFFENDER_VISITS.VISIT_INTERNAL_LOCATION_ID = AT_VISIT_LOCATION.INTERNAL_LOCATION_ID) AND (AT_PERSONS2.PERSON_ID = AT_OFFENDER_VISIT_VISITORS.PERSON_ID) AND (AT_OFFENDER_VISIT_VISITORS.OFFENDER_VISIT_ID = AT_OFFENDER_VISITS.OFFENDER_VISIT_ID) AND (AT_PERSON_ADDRESSES.OWNER_ID (+) = AT_PERSONS2.PERSON_ID) AND (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.LIVING_UNIT_ID = AT_AGENCY_INTERNAL_LOCATION_L4.INTERNAL_LOCATION_ID) AND (AT_OFFENDERS_LOCATIONS.AGENCY_LOCATION_TYPE = 'INST') AND (USER_ACCESSIBLE_CASELOADS.USERNAME = UPPER('SQZ39G')) AND (AT_OFFENDERS_LOCATIONS.AGY_LOC_ID = USER_ACCESSIBLE_CASELOADS.CASELOAD_ID) AND (AT_CASELOAD_LOCATIONS.AGY_LOC_ID = AT_OFFENDER_BOOKING.AGY_LOC_ID) AND (AT_CASELOAD_LOCATIONS.AGY_LOC_ID = AT_OFFENDERS_LOCATIONS.AGY_LOC_ID (+)) AND (NOT AT_CASELOAD_LOCATIONS.AGY_LOC_ID IN ('OUT', 'TRN') AND NOT AT_CASELOAD_LOCATIONS.CASELOAD_ID IN ('ADMINC', 'CADM_I', 'MULTI')) AND (AT_AGENCY_INTERNAL_LOCATION_L4.AGY_LOC_ID = AT_WING_ESTABLISHMENT.AGY_LOC_ID) AND (AT_WING_ESTABLISHMENT.AGY_LOC_ID = WING_CASELOAD_SECURITY.CASELOAD_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_VISITS.OFFENDER_BOOK_ID) AND (AT_OFFENDER_VISITS.VISIT_INTERNAL_LOCATION_ID = AT_VISIT_LOCATION.INTERNAL_LOCATION_ID) AND (AT_OFFENDER_VISITS.VISIT_STATUS = 'SCH') AND (USER_ACCESSIBLE_CASELOADS.USERNAME = UPPER('SQZ39G')) AND (WING_CASELOAD_SECURITY.USERNAME = UPPER('SQZ39G')) AND ((TRUNC(AT_OFFENDER_VISITS.VISIT_DATE) >= TO_DATE('28-08-2017 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TRUNC(AT_OFFENDER_VISITS.VISIT_DATE) <= TO_DATE('08-09-2017 00:00:00', 'DD-MM-YYYY HH24:MI:SS')) AND (AT_OFFENDERS_LOCATIONS.AGY_LOC_ID IN ('BFI') /* END Filter: Hierarchies/Establishment/''Establishment Code'' */) /* START Filter: Hierarchies/Establishment/''Establishment Code'' */ AND CASE WHEN AT_OFFENDER_VISITS.VISIT_TYPE = 'OFFI' THEN 'Official' ELSE 'Social/Family' END = 'Social/Family')), dataset_ AS (SELECT AGY_LOC_ID, SCHEDULED_VISIT_ESTABLISHMEN34, OFFENDER_ID_DISPLAY, LAST_NAME, FIRST_NAME, VPIS_FIRST_NAME, VPIS_LAST_NAME, VISIT_TYPE, PERSON_ID, BIRTHDATE, DESCRIPTION, VISIT_DATE, STREET, POSTAL_CODE, UNIT_DESCRIPTION_4_SHORT, VISIT_ORDER_NUMBER_LAST_5_DI32, BIRTH_DATE, START_TIME, END_TIME, COMMENT_TEXT, PRIMARY_FLAG, PREMISE, COUNTY_CODE, (COUNT(OFFENDER_ID_DISPLAY) OVER (PARTITION  BY PERSON_ID)) AS V_COUNT_PRISONER FROM dataset_base_)",
